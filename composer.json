{
  "name": "mindsize/laravel5-woocommerce",
  "description": "WooCommerce API Client for Laravel 5",
  "keywords": [
    "laravel",
    "wordpress",
    "woocommerce",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/Mindsize/laravel5-woocommerce",
  "license": "MIT",
  "authors": [
    {
      "name": "Mindsize",
      "email": "info@mindsize.me",
      "homepage": "https://mindsize.me/",
      "role": "Agency"
    },
    {
      "name": "Patrick Garman",
      "email": "patrick@mindsize.me",
      "homepage": "https://mindsize.me/",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "^7.0",
    "automattic/woocommerce": "1.3.*"
  },
  "autoload": {
    "psr-4": {
      "Mindsize\\WooCommerce\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mindsize\\WooCommerce\\Test\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^0.9.4",
    "satooshi/php-coveralls": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mindsize\\WooCommerce\\ServiceProvider"
      ],
      "aliases": {
        "WooCommerce": "Mindsize\\WooCommerce\\Facades\\WooCommerce"
      }
    }
  }
}
